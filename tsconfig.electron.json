{
  "extends": "./tsconfig.node.json",
  "compilerOptions": {
    "outDir": "dist-electron",
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["electron/**/*"]
}
